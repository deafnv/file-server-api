// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  ip_address  String
  username    String   @id @unique
  password    String
  rank        Int
  permissions String
  created_at  DateTime @default(now())
  jti         String
}
